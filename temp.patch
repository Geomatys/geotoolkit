+                        final String eUri = reader.getNamespaceURI();
+
+                        if (URI_KML.equals(eUri)) {
+                            if (TAG_KML.equals(eName)) {
+                                this.root = this.readKml();
+                            }
+                        }
+                        break;
+                }
+            }
+        } catch (XMLStreamException ex) {
