+        for(ViewRefreshMode cm : ViewRefreshMode.values()){
+            if(cm.getViewRefreshMode().equals(viewRefreshMode)){
+                resultat = cm;
+                break;
+            }
+        }
+        return resultat;
+    }
+
+}
