                    <gml:name codeSpace="AUTO">CalendarEra</gml:name>
                    <gml:remarks>calendar Era Remark</gml:remarks>
                    <gml:referenceEvent>blabla</gml:referenceEvent>
                    <gml:referenceDate>1900-01-01</gml:referenceDate>
                    <gml:julianReference>1578.000000000</gml:julianReference>
                    <gml:epochOfUse>
                        <gml:TimePeriod gml:id="auto-periodidentifier">
                            <gml:identifier codeSpace="AUTO">period identifier</gml:identifier>
                            <gml:name codeSpace="AUTO">period</gml:name>
                            <gml:begin>
                                <gml:TimeInstant gml:id="auto-start">
                                    <gml:identifier codeSpace="AUTO">start</gml:identifier>
                                    <gml:name codeSpace="AUTO">begiiinn hou hou hou</gml:name>
                                    <gml:timePosition>2014-10-22T11:59:35.455+02:00</gml:timePosition>
