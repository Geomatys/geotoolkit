    <gml:utcReference>00:00:00</gml:utcReference>
    <gml:dateBasis>
        <gml:TimeCalendar gml:id="auto-referenceSystem-calendaridentifier">
            <gml:identifier codeSpace="AUTO">urn:ogc:def:referenceSystem:AUTO::Calendaridentifier</gml:identifier>
