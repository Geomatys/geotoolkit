                            </gml:begin>
                            <gml:end>
                                <gml:TimeInstant gml:id="auto-end">
                                    <gml:identifier codeSpace="AUTO">end</gml:identifier>
                                    <gml:name codeSpace="AUTO">happy ending</gml:name>
                                    <gml:timePosition>2014-10-22T11:59:35.458+02:00</gml:timePosition>
                                </gml:TimeInstant>
                            </gml:end>
                        </gml:TimePeriod>
                    </gml:epochOfUse>
                </gml:TimeCalendarEra>
            </gml:referenceFrame>
            <gml:referenceFrame>
                <gml:TimeCalendarEra gml:id="auto-calendarera2identifier">
                    <gml:identifier codeSpace="AUTO">CalendarEra2 identifier</gml:identifier>
                    <gml:name codeSpace="AUTO">CalendarEra2</gml:name>
                    <gml:remarks>calendar Era Remark2</gml:remarks>
                    <gml:referenceEvent>blabla2</gml:referenceEvent>
